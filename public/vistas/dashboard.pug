extends layout
block links
    ul.navbar-nav.mr-auto
        li.nav-item.active
            a.nav-link.waves-effect(href="/dashboard") 
                i.fa.fa-home.ml-1
                span Inicio
        li.nav-item
            a.nav-link.waves-effect(href="/perfil")
                i.fa.fa-user.ml-1
                span Perfil
block contenido
  if (message && message.length > 0) 
    div.alert.alert-success.alert-dismissible.fade.show#contenedor-mensaje(style = "display:none;" role="alert") #{message} Bienvenido(a) #{nombreUsuario}
      button.close(type="button" data-dismiss="alert" aria-label="Close")
        span(aria-hidden="true") &times;
  div.card.mb-4.wow.fadeIn
    div.card-body.d-sm-flex.justify-content-between
      h3.mb-2.mb-sm-0.pt-1 Dashboard
        //h3= "Bienvenido " + nombreUsuario 
block scripts   
    script(type="text/javascript" src="js/jquery-3.3.1.min.js")
    script(type="text/javascript" src="js/bootstrap.min.js")
    script(type="text/javascript" src="js/mdb.min.js")
    script(type="text/javascript" src="js/activar.js")
